    "# Kasus 3b Mau hitung apa?\n",
    "import math\n",
    "\n",
    "def luas_persegi_panjang(panjang,lebar):\n",
    "    luas_persegi_panjang = panjang * lebar\n",
    "    return luas_persegi_panjang\n",
    "\n",
    "def keliling_persegi_panjang(panjang,lebar):\n",
    "    keliling_persegi_panjang = 2 * ( panjang + lebar )\n",
    "    return keliling_persegi_panjang\n",
    "\n",
    "def panjang_diagonal_persegi_panjang(panjang,lebar):\n",
    "    panjang_diagonal_persegi_panjang = math.sqrt(panjang**2 + lebar**2)\n",
    "    return panjang_diagonal_persegi_panjang\n",
    "\n",
    "yang_kamu_cari = input(\"Kamu mau hitung luas / keliling / panjang diagonal?: \")\n",
    "a = int(input(\"masukkan panjang:\"))\n",
    "b = int(input(\"masukkan lebar:\"))\n",
    "if (yang_kamu_cari == 'luas'):\n",
    "    print (\"luas persegi panjang dengan panjang dan lebar\",a,\",\",b,\"adalah\",luas_persegi_panjang(a,b),\"satuan luas\")\n",
    "elif (yang_kamu_cari == 'keliling'):\n",
    "    print (\"keliling persegi panjang dengan panjang dan lebar\",a,\",\",b,\"adalah\",keliling_persegi_panjang(a,b),\"satuan\")\n",
    "elif (yang_kamu_cari == 'panjang diagonal'):\n",
    "    print (\"panjang diagonal persegi panjang dengan panjang dan lebar\",a,\",\",b,\"adalah\",panjang_diagonal_persegi_panjang(a,b),\"satuan\")\n",
    "else:\n",
    "    print ('inputanmu ada yang salah boss')"
